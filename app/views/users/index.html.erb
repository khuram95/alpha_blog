<h1 align="center">user page</h1>
<div class="center">
    <%= will_paginate %>
</div>
<% @users.each do |u|%>
    <ul class="listing">
         <div class="row center">
            <div class="well col-md-4 col-md-offset-4">
                <li><%= link_to gravatar_for(u), user_path(u) %></li>
                <li class="article-title">
                <%= link_to u.username, user_path(u) %>
                </li>
                <li><small><%= pluralize(u.articals.count, "article") if u.articals %></small></li>
                <% if logged_in? and current_user.admin? %>
                    <li><%= link_to "Delete this user", user_path(u), method: :delete,
                    data: { confirm: "Are you sure you want to delete the user and all their articles?" } %></li>
                <% end %>
            </div>
        </div>   
    </ul>
 <%end%>
<div class="center">
    <%= will_paginate %>
</div>